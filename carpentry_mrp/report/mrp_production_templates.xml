<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="report_mrporder" inherit_id="mrp.report_mrporder">
        <!-- Add `description` -->
        <h2 position="replace">
            <h2>
                <span t-field="o.name" />
                <t t-if="o.description">
                    - <span t-field="o.description" />
                </t>
            </h2>
        </h2>
            
        <!-- Add project -->
        <h2 position="after">
            <h5 t-if="o.project_id">
                Project : <span t-field="o.project_id" />
            </h5>
            <h5 t-if="o.launch_ids">
                Launch(s) : <span t-field="o.launch_ids" />
            </h5>
        </h2>
    </template>
</odoo>
