<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="report_mrporder" inherit_id="mrp.report_mrporder">
        <!-- Add `description` -->
        <span t-field="o.name" position="attributes">
            <attribute name="t-field">o.display_name</attribute>
        </span>
            
        <!-- Add project -->
        <h2 position="after">
            <h5 t-if="o.project_id">
                Project : <span t-field="o.project_id" />
            </h5>
            <h5 t-if="o.launch_ids">
                Launch(s) : <span t-field="o.launch_ids" />
            </h5>
        </h2>
    </template>
</odoo>
